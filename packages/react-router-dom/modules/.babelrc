  "plugins": [
    "dev-expression",
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "transform-export-extensions"
  ],
  "env": {
    "test": {
      "presets": [["env", { "loose": true }], "react"],
      "plugins": [
